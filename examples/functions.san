func createAnimatedCircle(x, r) 
	c = Create Circle {x,y} color=blue // <svg Cirle
	

	@begin=0, duration=2
	Anim1 = Translate c {10,0}  

	@begin=0, duration=2
	Modify c color=green

	@begin=2, duration=4
	Translate c {0,10}
endfunc

func main()
	r = Create Rectangle {x,y}	// /svg> <svg Rectangle 
	for (i in 1:10)
		createAnimatedCircle(i,r)
	endfor
endfunc
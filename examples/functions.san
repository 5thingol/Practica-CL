func createAnimatedCircle(x, r)
	c = Create Circle {x,y} color=blue; // <svg Cirle
	
	@begin=0, duration=2;
	Anim1 = Translate c {10,0};

	@begin=0, duration=2;
	Modify c color=green;

	@begin=2, duration=4;
	Translate c {0,10};
endfunc

func main()
	x = 10;
	y = 20;
	r = Create Rectangle {x,y};	// <svg Cirle
	@begin=0, end=1;
	Translate r {3,3}; 
	r = Create Rectangle {10,20};

	for (i in 1:10)
		createAnimatedCircle(i,r);
	endfor;
endfunc